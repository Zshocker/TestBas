@page
@model TestBase.Pages.Students.CreateModel

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>Etudiant</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post" enctype="multipart/form-data">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Etudiant.name" class="control-label"></label>
                <input asp-for="Etudiant.name" class="form-control" />
                <span asp-validation-for="Etudiant.name" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Etudiant.CNE" class="control-label"></label>
                <input asp-for="Etudiant.CNE" class="form-control" />
                <span asp-validation-for="Etudiant.CNE" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="file" class="control-label">Image</label>
                <input asp-for="file" type="file"  class="custom-file" />
                <span asp-validation-for="file" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-page="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
